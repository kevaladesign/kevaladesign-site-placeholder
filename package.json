{
  "name": "gatsby-ssg-template",
  "private": true,
  "description": "Static site generator template using Gatsby",
  "version": "0.1.0",
  "author": "Kevala Design <kevaladesignco@gmail.com>",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.64",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-emotion": "^4.3.11",
    "gatsby-plugin-manifest": "^2.4.31",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-sharp": "^2.5.15",
    "immer": "^7.0.9",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^1.8.10",
    "twin.macro": "^1.9.0",
    "use-immer": "^0.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.10",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.0",
    "eslint-plugin-unicorn": "^22.0.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "msw": "^0.21.2",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.0.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:update-snapshots": "jest -u",
    "tsc:check": "tsc --noEmit",
    "eslint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier:check": "prettier --list-different \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write"
    }
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "emotion",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevaladesign/gatsby-ssg-template"
  },
  "bugs": {
    "url": "https://github.com/kevaladesign/gatsby-ssg-template/issues"
  }
}
